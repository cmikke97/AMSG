name: AMSG_workflow

conda_env: conda.yaml

entry_points:
  download_dataset:
    parameters:
      destination_dir: path
    command: "python sorel20mDownloader.py sorel20m_download --destination_dir {destination_dir}"

  main:
    parameters:
      base_dir: path
    command: "python main.py workflow --base_dir {base_dir}"