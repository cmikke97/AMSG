name: AMSG_workflow

conda_env: conda.yaml

entry_points:
  download_dataset:
    parameters:
      destination_dir: path
    command: "python src/DatasetDownloader/sorel20mDownloader.py --destination_dir {destination_dir}"

  main:
    parameters:
      base_dir: path
    command: "python src/main.py --base_dir {base_dir}"